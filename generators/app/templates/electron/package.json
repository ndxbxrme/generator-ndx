{
  "name": "<%= settings.appName %>",
  "version": "0.0.1",
  "description": "<%= settings.description %>",
  "main": "app/main.js",
  "scripts": {
    "start": "electron .",
    "pack-win": "build --win -p always",
    "pack-mac": "build --mac -p always"
  },
  "build": {
    "appId": "com.<%=settings.gitname%>.<%= settings.appName %>",
    "productName": "<%= settings.appName %>"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/<%=settings.gitname%>/<%= settings.appName %>"
  },
  "author": "<%=settings.gitname%>",
  "license": "MIT",
  "homepage": "https://github.com/<%=settings.gitname%>/<%= settings.appName %>"
}
